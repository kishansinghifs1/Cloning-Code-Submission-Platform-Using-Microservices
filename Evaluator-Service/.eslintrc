{
    "parser": "@typescript-eslint/parser",
    "extends": [
        "plugin:@typescript-eslint/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "simple-import-sort/imports": "error",
        "simple-import-sort/exports": "error"
    },
    "plugins": [
        "simple-import-sort"
    ]
}